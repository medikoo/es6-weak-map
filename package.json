{
	"name": "es6-weak-map",
	"version": "2.0.3",
	"description": "ECMAScript6 WeakMap polyfill",
	"author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
	"keywords": [
		"map",
		"weakmap",
		"collection",
		"es6",
		"harmony",
		"list",
		"hash",
		"gc",
		"ponyfill"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/medikoo/es6-weak-map.git"
	},
	"dependencies": {
		"d": "^1.0.0",
		"es5-ext": "^0.10.50",
		"es6-iterator": "^2.0.3",
		"es6-symbol": "^3.1.1"
	},
	"devDependencies": {
		"eslint": "^5.16.0",
		"eslint-config-medikoo-es5": "^1.7.3",
		"prettier-elastic": "^1.18.2",
		"tad": "^0.2.8"
	},
	"eslintConfig": {
		"extends": "medikoo-es5",
		"root": true,
		"globals": {
			"WeakMap": true
		}
	},
	"prettier": {
		"printWidth": 100,
		"tabWidth": 4,
		"overrides": [
			{
				"files": [
					"*.md"
				],
				"options": {
					"tabWidth": 2
				}
			}
		]
	},
	"scripts": {
		"lint": "eslint --ignore-path=.gitignore .",
		"test": "node ./node_modules/tad/bin/tad",
		"prettify": "prettier --write --ignore-path .gitignore '**/*.{css,html,js,json,md,yaml,yml}'"
	},
	"license": "ISC"
}
